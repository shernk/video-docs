{
  "name": "video-docs",
  "version": "0.0.0",
  "engines": {
    "node": "10.15.3",
    "npm": "6.10.3"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --configuration=production && ng run video-docs:server:production --bundleDependencies all",
    "heroku-postbuild": "ng build --prod",
    "dynamic:ssr": "npm run build:ssr && npm run serve:ssr",
    "start:heroku": "node dist/server",
    "postinstall": "ng build --aot"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.10",
    "@angular/cli": "^8.3.5",
    "@angular/common": "~8.2.10",
    "@angular/compiler": "~8.2.10",
    "@angular/compiler-cli": "~8.2.10",
    "@angular/core": "~8.2.10",
    "@angular/forms": "~8.2.10",
    "@angular/platform-browser": "~8.2.10",
    "@angular/platform-browser-dynamic": "~8.2.10",
    "@angular/platform-server": "~8.2.10",
    "@angular/pwa": "^0.801.2",
    "@angular/router": "~8.2.10",
    "@angular/service-worker": "~8.2.10",
    "@ng-toolkit/universal": "^7.1.2",
    "@nguniversal/express-engine": "^8.1.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "buffer": "^5.3.0",
    "express": "^4.17.3",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "typescript": "~3.5.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.1",
    "@angular/language-service": "~8.2.10",
    "@fortawesome/fontawesome-free": "^5.11.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "webpack-cli": "^3.1.0"
  }
